{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2018",
        "lib": [
           "es5", "es6", "es2017","es2018", "dom", "dom.Iterable", "scripthost", "es2015.iterable"
        ],
        "sourceMap": true,
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noUnusedLocals": false,
       "noUnusedParameters": false,
      "outDir": "dist",
       "baseUrl": ".",
       "paths": {
           }
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
               "dist",
               "node_modules",
               "typings"
    ]
}
